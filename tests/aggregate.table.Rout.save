> g1 <- sample(letters[1:5], 1000, replace=TRUE)
> g2 <- sample(LETTERS[1:3], 1000, replace=TRUE )
> dat <- rnorm(1000)
> 
> stderr <- function(x) sqrt( var(x,na.rm=TRUE) / (nobs(x) - 1 ) )
